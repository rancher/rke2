{
	"linters": {
		"disable-all": true,
		"enable": [
			"govet",
			"revive",
			"goimports",
			"misspell",
			"ineffassign",
			"gofmt"
		]
	},
	"run": {
		"skip-files": [
			"/zz_generated_"
		],
		"deadline": "5m"
	},
	"issues": {
		"exclude-rules": [
			{
				"linters": "typecheck",
				"text": "imported but not used"
			}
		]
	}
}
